{
  "name": "bandit",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "ajv": "^6.5.2",
    "amazon-cognito-identity-js": "^2.0.13",
    "aphrodite": "^2.2.2",
    "aws-amplify": "0.4.8",
    "aws-amplify-react": "^0.1.54",
    "bootstrap": "^4.1.1",
    "graphql": "^0.13.0",
    "husky": "^0.14.3",
    "jquery": "^3.3.1",
    "lint-staged": "^7.2.0",
    "moment": "^2.22.2",
    "node-sass-chokidar": "^1.3.3",
    "normalize.css": "^8.0.0",
    "npm": "^6.3.0",
    "npm-run-all": "^4.1.3",
    "popper.js": "^1.14.3",
    "prettier": "^1.13.5",
    "react": "^16.4.1",
    "react-datetime": "^2.15.0",
    "react-dom": "^16.4.1",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.4",
    "reactstrap": "^6.2.0",
    "redux": "^4.0.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "scripts": {
    "precommit": "lint-staged",
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "predeploy": "npm run build",
    "deploy": "aws s3 sync build/ s3://bandit-file-storage --delete",
    "postdeploy": "aws cloudfront create-invalidation --distribution-id E11WI1HV1G0VO --paths '/*' && aws cloudfront create-invalidation --distribution-id E84A326ZKGQ5W --paths '/*'",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "redux-devtools": "^3.4.1",
    "webpack": "^3.12.0"
  }
}
